{
  "id": null,
  "uid": "materio-socket",
  "title": "Materio Socket.IO Overview",
  "description": "Socket.IO monitoring: connections, messages, rooms, performance",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": ["socket", "websocket", "realtime"],
  "templating": {
    "list": [
      {
        "name": "environment",
        "type": "custom",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "options": [
          { "selected": true, "text": "All", "value": "$__all" },
          { "selected": false, "text": "development", "value": "development" },
          { "selected": false, "text": "production", "value": "production" }
        ],
        "includeAll": true,
        "multi": false
      },
      {
        "name": "namespace",
        "type": "custom",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "options": [
          { "selected": true, "text": "All", "value": "$__all" },
          { "selected": false, "text": "/", "value": "/" },
          { "selected": false, "text": "/chat", "value": "/chat" },
          { "selected": false, "text": "/notifications", "value": "/notifications" }
        ],
        "includeAll": true,
        "multi": false
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "üìä Connection Status",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Active Connections",
      "description": "Current active Socket.IO connections",
      "gridPos": { "x": 0, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 100 },
              { "color": "red", "value": 500 }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(socket_active_connections{environment=~\"$environment\", namespace=~\"$namespace\"})",
          "legendFormat": "Connections",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Active Users",
      "description": "Unique active users",
      "gridPos": { "x": 4, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(socket_active_users{environment=~\"$environment\", namespace=~\"$namespace\"})",
          "legendFormat": "Users",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Active Rooms",
      "description": "Active chat/notification rooms",
      "gridPos": { "x": 8, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "purple", "value": null }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(socket_active_rooms{environment=~\"$environment\", namespace=~\"$namespace\"})",
          "legendFormat": "Rooms",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Connections (5m)",
      "description": "New connections in last 5 minutes",
      "gridPos": { "x": 12, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(increase(socket_connections_total{environment=~\"$environment\", namespace=~\"$namespace\", status=\"success\"}[5m]))",
          "legendFormat": "Connections",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Errors (5m)",
      "description": "Socket.IO errors in last 5 minutes",
      "gridPos": { "x": 16, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(increase(socket_errors_total{environment=~\"$environment\", namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "Errors",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Server Uptime",
      "description": "Socket.IO server uptime",
      "gridPos": { "x": 20, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          },
          "unit": "s"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "socket_server_uptime_seconds{environment=~\"$environment\"}",
          "legendFormat": "Uptime",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "üìà Connections",
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Active Connections Over Time",
      "description": "Active connections by namespace",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "socket_active_connections{environment=~\"$environment\", namespace=~\"$namespace\"}",
          "legendFormat": "{{namespace}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Connection Rate",
      "description": "Connections and disconnections per minute",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(socket_connections_total{environment=~\"$environment\", namespace=~\"$namespace\", status=\"success\"}[5m])) * 60",
          "legendFormat": "Connections",
          "refId": "A"
        },
        {
          "expr": "sum(rate(socket_disconnects_total{environment=~\"$environment\", namespace=~\"$namespace\"}[5m])) * 60",
          "legendFormat": "Disconnections",
          "refId": "B"
        }
      ]
    },
    {
      "type": "row",
      "title": "üí¨ Messages",
      "gridPos": { "x": 0, "y": 14, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Messages Rate",
      "description": "Messages per minute by direction",
      "gridPos": { "x": 0, "y": 15, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(socket_messages_total{environment=~\"$environment\", namespace=~\"$namespace\"}[5m])) by (direction) * 60",
          "legendFormat": "{{direction}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Messages by Event",
      "description": "Messages per minute by event type",
      "gridPos": { "x": 12, "y": 15, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(socket_messages_total{environment=~\"$environment\", namespace=~\"$namespace\"}[5m])) by (event) * 60",
          "legendFormat": "{{event}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "‚ö° Performance",
      "gridPos": { "x": 0, "y": 23, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Message Duration (p50, p95, p99)",
      "description": "Message handling latency percentiles",
      "gridPos": { "x": 0, "y": 24, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(socket_message_duration_seconds_bucket{environment=~\"$environment\", namespace=~\"$namespace\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(socket_message_duration_seconds_bucket{environment=~\"$environment\", namespace=~\"$namespace\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(socket_message_duration_seconds_bucket{environment=~\"$environment\", namespace=~\"$namespace\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Message Size",
      "description": "Average message size by direction",
      "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "bytes"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(socket_message_size_bytes_bucket{environment=~\"$environment\", namespace=~\"$namespace\"}[5m])) by (le, direction))",
          "legendFormat": "{{direction}} p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "üîê Authentication",
      "gridPos": { "x": 0, "y": 32, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Auth Events",
      "description": "Authentication events over time",
      "gridPos": { "x": 0, "y": 33, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(socket_auth_events_total{environment=~\"$environment\"}[5m])) by (status) * 60",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "piechart",
      "title": "Auth Success Rate",
      "description": "Distribution of authentication results",
      "gridPos": { "x": 12, "y": 33, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
        "pieType": "pie",
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "sum(increase(socket_auth_events_total{environment=~\"$environment\"}[1h])) by (status)",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "üö® Errors & Disconnects",
      "gridPos": { "x": 0, "y": 41, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Errors by Type",
      "description": "Socket.IO errors over time",
      "gridPos": { "x": 0, "y": 42, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(socket_errors_total{environment=~\"$environment\", namespace=~\"$namespace\"}[5m])) by (error_type) * 60",
          "legendFormat": "{{error_type}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Disconnects by Reason",
      "description": "Disconnection reasons over time",
      "gridPos": { "x": 12, "y": 42, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(socket_disconnects_total{environment=~\"$environment\", namespace=~\"$namespace\"}[5m])) by (reason) * 60",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ]
    }
  ]
}


