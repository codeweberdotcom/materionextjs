{
  "id": null,
  "uid": "materio-redis",
  "title": "Materio Redis Overview",
  "description": "Redis monitoring: connections, operations, performance, memory",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": ["redis", "database", "cache"],
  "templating": {
    "list": [
      {
        "name": "environment",
        "type": "custom",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "options": [
          { "selected": true, "text": "All", "value": "$__all" },
          { "selected": false, "text": "development", "value": "development" },
          { "selected": false, "text": "production", "value": "production" }
        ],
        "includeAll": true,
        "multi": false
      },
      {
        "name": "instance",
        "type": "custom",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "options": [
          { "selected": true, "text": "All", "value": "$__all" },
          { "selected": false, "text": "default", "value": "default" },
          { "selected": false, "text": "cache", "value": "cache" },
          { "selected": false, "text": "session", "value": "session" }
        ],
        "includeAll": true,
        "multi": false
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "ðŸ“Š Connection Status",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Connection Status",
      "description": "Redis connection status (1 = connected)",
      "gridPos": { "x": 0, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "1": { "text": "ðŸŸ¢ Connected", "color": "green" } } },
            { "type": "value", "options": { "0": { "text": "ðŸ”´ Disconnected", "color": "red" } } }
          ],
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "redis_connection_status{environment=~\"$environment\", instance=~\"$instance\"}",
          "legendFormat": "Status",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Active Connections",
      "description": "Number of active Redis connections",
      "gridPos": { "x": 4, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(redis_active_connections{environment=~\"$environment\", instance=~\"$instance\"})",
          "legendFormat": "Connections",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Connection Errors (5m)",
      "description": "Redis connection errors in last 5 minutes",
      "gridPos": { "x": 8, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(increase(redis_connection_errors_total{environment=~\"$environment\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "Errors",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Reconnections (5m)",
      "description": "Redis reconnection attempts in last 5 minutes",
      "gridPos": { "x": 12, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "orange", "value": 5 }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(increase(redis_reconnections_total{environment=~\"$environment\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "Reconnections",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Uptime",
      "description": "Redis server uptime",
      "gridPos": { "x": 16, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          },
          "unit": "s"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "redis_uptime_seconds{environment=~\"$environment\", instance=~\"$instance\"}",
          "legendFormat": "Uptime",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Total Keys",
      "description": "Total number of keys in Redis",
      "gridPos": { "x": 20, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(redis_keys_total{environment=~\"$environment\", instance=~\"$instance\"})",
          "legendFormat": "Keys",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "ðŸ“ˆ Operations",
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Operations Rate",
      "description": "Redis operations per minute by type",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(redis_operations_total{environment=~\"$environment\", instance=~\"$instance\"}[5m])) by (operation) * 60",
          "legendFormat": "{{operation}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Operations by Status",
      "description": "Success vs Error operations",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(redis_operations_total{environment=~\"$environment\", instance=~\"$instance\"}[5m])) by (status) * 60",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "âš¡ Performance",
      "gridPos": { "x": 0, "y": 14, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Operation Duration (p50, p95, p99)",
      "description": "Redis operation latency percentiles",
      "gridPos": { "x": 0, "y": 15, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(redis_operation_duration_seconds_bucket{environment=~\"$environment\", instance=~\"$instance\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(redis_operation_duration_seconds_bucket{environment=~\"$environment\", instance=~\"$instance\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(redis_operation_duration_seconds_bucket{environment=~\"$environment\", instance=~\"$instance\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Duration by Operation",
      "description": "Average operation duration by type",
      "gridPos": { "x": 12, "y": 15, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(redis_operation_duration_seconds_bucket{environment=~\"$environment\", instance=~\"$instance\"}[5m])) by (le, operation))",
          "legendFormat": "{{operation}} p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "ðŸ’¾ Memory & Storage",
      "gridPos": { "x": 0, "y": 23, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Memory Usage",
      "description": "Redis memory usage over time",
      "gridPos": { "x": 0, "y": 24, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "bytes"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "redis_memory_usage_bytes{environment=~\"$environment\", instance=~\"$instance\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Keys Count",
      "description": "Number of keys over time",
      "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "redis_keys_total{environment=~\"$environment\", instance=~\"$instance\"}",
          "legendFormat": "{{instance}} db{{database}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "ðŸ”§ Commands",
      "gridPos": { "x": 0, "y": 32, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Commands Rate",
      "description": "Redis commands per minute by type",
      "gridPos": { "x": 0, "y": 33, "w": 16, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(redis_commands_total{environment=~\"$environment\", instance=~\"$instance\"}[5m])) by (command) * 60",
          "legendFormat": "{{command}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "piechart",
      "title": "Commands Distribution",
      "description": "Distribution of Redis commands",
      "gridPos": { "x": 16, "y": 33, "w": 8, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
        "pieType": "pie",
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "sum(increase(redis_commands_total{environment=~\"$environment\", instance=~\"$instance\"}[1h])) by (command)",
          "legendFormat": "{{command}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "ðŸš¨ Errors",
      "gridPos": { "x": 0, "y": 41, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Connection Errors",
      "description": "Redis connection errors over time",
      "gridPos": { "x": 0, "y": 42, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(redis_connection_errors_total{environment=~\"$environment\", instance=~\"$instance\"}[5m])) by (error_type) * 60",
          "legendFormat": "{{error_type}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Failed Operations",
      "description": "Failed Redis operations over time",
      "gridPos": { "x": 12, "y": 42, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(redis_operations_total{environment=~\"$environment\", instance=~\"$instance\", status=\"error\"}[5m])) by (operation) * 60",
          "legendFormat": "{{operation}}",
          "refId": "A"
        }
      ]
    }
  ]
}

