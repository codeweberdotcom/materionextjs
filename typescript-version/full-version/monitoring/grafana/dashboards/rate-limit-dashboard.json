{
  "id": null,
  "uid": "materio-rl",
  "title": "Materio Rate Limit Overview",
  "description": "Rate limiting metrics: checks, blocks, fallback, performance",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 2,
  "refresh": "10s",
  "tags": ["rate-limit", "security"],
  "templating": {
    "list": [
      {
        "name": "environment",
        "type": "custom",
        "current": {
          "selected": true,
          "text": "All",
          "value": "$__all"
        },
        "options": [
          { "selected": true, "text": "All", "value": "$__all" },
          { "selected": false, "text": "development", "value": "development" },
          { "selected": false, "text": "production", "value": "production" }
        ],
        "includeAll": true,
        "multi": false
      }
    ]
  },
  "panels": [
    {
      "type": "row",
      "title": "ðŸ“Š Overview Stats",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "stat",
      "title": "Redis Failures (5m)",
      "description": "Number of Redis failures that triggered fallback",
      "gridPos": { "x": 0, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(increase(rate_limit_redis_failures_total{environment=~\"$environment\"}[5m]))",
          "legendFormat": "Redis Failures",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Unknown Modules (5m)",
      "description": "Rate limit checks for modules without config",
      "gridPos": { "x": 4, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "orange", "value": 10 }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(increase(rate_limit_unknown_module_total{environment=~\"$environment\"}[5m]))",
          "legendFormat": "Unknown Modules",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Total Checks (5m)",
      "description": "Total rate limit checks performed",
      "gridPos": { "x": 8, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(increase(rate_limit_checks_total{environment=~\"$environment\"}[5m]))",
          "legendFormat": "Total Checks",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Blocked (5m)",
      "description": "Requests blocked by rate limiting",
      "gridPos": { "x": 12, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 100 }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(increase(rate_limit_checks_total{environment=~\"$environment\", result=\"blocked\"}[5m]))",
          "legendFormat": "Blocked",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Active Blocks",
      "description": "Currently active blocks count",
      "gridPos": { "x": 16, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "sum(rate_limit_active_blocks{environment=~\"$environment\"})",
          "legendFormat": "Active Blocks",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Backend",
      "description": "Current backend (Redis or Prisma)",
      "gridPos": { "x": 20, "y": 1, "w": 4, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "1": { "text": "ðŸŸ¢ Redis", "color": "green" } } },
            { "type": "value", "options": { "0": { "text": "ðŸŸ¡ Prisma", "color": "yellow" } } }
          ],
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      },
      "targets": [
        {
          "expr": "rate_limit_store_backend{backend=\"redis\", environment=~\"$environment\"}",
          "legendFormat": "Backend",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "ðŸ“ˆ Backend & Fallback",
      "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Backend Status",
      "description": "Which backend is active over time",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "rate_limit_store_backend{environment=~\"$environment\"}",
          "legendFormat": "{{backend}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Fallback Switches",
      "description": "Backend switches between Redis and Prisma",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "increase(rate_limit_fallback_switch_total{environment=~\"$environment\"}[5m])",
          "legendFormat": "{{from}} â†’ {{to}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "âš¡ Performance",
      "gridPos": { "x": 0, "y": 14, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Consume Duration (p50, p95, p99)",
      "description": "Time spent in store.consume()",
      "gridPos": { "x": 0, "y": 15, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(rate_limit_consume_duration_seconds_bucket{environment=~\"$environment\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(rate_limit_consume_duration_seconds_bucket{environment=~\"$environment\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(rate_limit_consume_duration_seconds_bucket{environment=~\"$environment\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Check Duration (p50, p95, p99)",
      "description": "Time spent in RateLimitEngine.checkLimit()",
      "gridPos": { "x": 12, "y": 15, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(rate_limit_check_duration_seconds_bucket{environment=~\"$environment\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(rate_limit_check_duration_seconds_bucket{environment=~\"$environment\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(rate_limit_check_duration_seconds_bucket{environment=~\"$environment\"}[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "type": "row",
      "title": "ðŸ”’ Rate Limiting",
      "gridPos": { "x": 0, "y": 23, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Checks by Module",
      "description": "Rate limit checks per module (allowed vs blocked)",
      "gridPos": { "x": 0, "y": 24, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(rate_limit_checks_total{environment=~\"$environment\"}[5m])) by (module, result) * 60",
          "legendFormat": "{{module}} ({{result}})",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Events (Warnings & Blocks)",
      "description": "Rate limit events over time",
      "gridPos": { "x": 12, "y": 24, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(rate_limit_events_total{environment=~\"$environment\"}[5m])) by (event_type) * 60",
          "legendFormat": "{{event_type}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "ðŸš« Blocks",
      "gridPos": { "x": 0, "y": 32, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Blocks by Type",
      "description": "Blocks distribution: automatic, manual, user, ip, email, domain",
      "gridPos": { "x": 0, "y": 33, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "short"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "sum(rate(rate_limit_blocks_total{environment=~\"$environment\"}[5m])) by (block_type) * 60",
          "legendFormat": "{{block_type}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "piechart",
      "title": "Active Blocks Distribution",
      "description": "Currently active blocks by type",
      "gridPos": { "x": 12, "y": 33, "w": 12, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
        "pieType": "pie",
        "tooltip": { "mode": "single" }
      },
      "targets": [
        {
          "expr": "sum(rate_limit_active_blocks{environment=~\"$environment\"}) by (block_type)",
          "legendFormat": "{{block_type}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "row",
      "title": "ðŸ“‹ Fallback Duration",
      "gridPos": { "x": 0, "y": 41, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "type": "timeseries",
      "title": "Fallback Duration (time in Prisma)",
      "description": "Duration the service stays in Prisma fallback",
      "gridPos": { "x": 0, "y": 42, "w": 24, "h": 8 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "s"
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(rate_limit_fallback_duration_seconds_bucket{environment=~\"$environment\"}[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(rate_limit_fallback_duration_seconds_bucket{environment=~\"$environment\"}[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        }
      ]
    }
  ]
}
